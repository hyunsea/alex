

<div class="container">
            <form action="/update/<%= @one_post.id %>" method="POST">
                <div class="form-group">
                    <label for="exampleInputEmail1">제목</label>
                    <input type="text" name="title" value="<%=@one_post.title %>" class="form-control"
                    id="exampleInputEmail1" placeholder="제목">
                </div>
                <label for="exampleInputEmail1">내용</label>
                <textarea name="content" class="form-control" rows="5" id="commit"><%=@one_post.content %></textarea>
                <br>
                <button type="submit" class="btn btn-default" style="margintop:
                20px">전송</button>
            </form>
</div>